{
  "name": "botboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "prisma generate",
    "start": "node build/server.js",
    "build": "vite -c ./vite.server.ts build && vite -c ./vite.app.ts build",
    "dev": "concurrently -P -n app,srv -c \"magenta,green\" \"vite -c ./vite.app.ts {@}\" \"vite -c ./vite.server.ts {@}\" --",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db": "prisma studio"
  },
  "dependencies": {
    "@lucia-auth/adapter-prisma": "^3.0.2",
    "@lucia-auth/oauth": "^3.3.1",
    "@prisma/client": "5.4.1",
    "@trpc/server": "^10.38.5",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucia": "^2.7.2",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.13",
