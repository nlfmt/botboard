{
    "name": "botboard",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "start": "node dist/server.js",
        "build": "vite build && tsc -p tsconfig.server.json",
        "dev": "concurrently \"vite --port 3000\" \"tsc --watch --preserveWatchOutput -p tsconfig.server.json\" \"nodemon --watch dist dist/server.js\"",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "db:push": "prisma db push",
        "db:generate": "prisma generate",
        "db:studio": "prisma studio"
    },
    "dependencies": {
        "@prisma/client": "5.3.1",
        "@tanstack/react-query": "^4.35.7",
        "@trpc/client": "^10.38.5",
        "@trpc/react-query": "^10.38.5",
        "@trpc/server": "^10.38.5",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "morgan": "^1.10.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "zod": "^3.22.2"
    },
    "devDependencies": {
        "@types/cors": "^2.8.14",
        "@types/express": "^4.17.18",
        "@types/morgan": "^1.9.6",
        "@types/node": "^20.8.2",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-react-swc": "^3.3.2",
        "chokidar": "^3.5.3",
        "concurrently": "^8.2.1",
        "eslint": "^8.45.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "nodemon": "^3.0.1",
        "prisma": "^5.3.1",
        "sass": "^1.68.0",
        "typescript": "^5.2.2",
        "vite": "^4.4.5"
    }
}
