    "eslint-plugin-react-refresh": "^0.4.3",
    "prisma": "^5.4.1",
    "react-router-dom": "^6.17.0",
    "sass": "^1.68.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "vite-plugin-node": "^3.0.2",
    "vite-plugin-pwa": "^0.16.7",
